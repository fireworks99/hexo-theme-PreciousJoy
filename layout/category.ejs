
<div id="category">
  <h1><%=page.category%></h1>
  <% page.posts.each(article => { %>
  <div class="category-item">
    <a href="<%=url_for(article.path)%>">
      <%=article.title%>
    </a>
    <time datetime="<%=date_xml(article.date)%>">
      <%=date(article.date)%>
    </time>
  </div>
  <% }); %>

  <div id="other-paginator">
    <%-paginator()%>
  </div>

</div>


